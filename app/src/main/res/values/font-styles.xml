<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextView" parent="Widget.MaterialComponents.TextView">
        <item name="android:enabled">true</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextView.Regular">
        <item name="fontFamily">@font/regular_font</item>
        <item name="android:fontFamily">@font/regular_font</item>
    </style>

    <style name="TextView.Bold">
        <item name="fontFamily">@font/bold_font</item>
        <item name="android:fontFamily">@font/bold_font</item>
    </style>

    <style name="TextView.ExtraBold">
        <item name="fontFamily">@font/extra_bold_font</item>
        <item name="android:fontFamily">@font/extra_bold_font</item>
    </style>

    <style name="TextView.Regular.10">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TextView.Regular.12">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TextView.Bold.10">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TextView.Bold.12">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TextView.ExtraBold.10">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TextView.ExtraBold.12">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TextView.ExtraBold.14">
        <item name="android:textSize">14sp</item>
    </style>
</resources>